{
  "buildCommand": "cd dashboard && pnpm install && pnpm build",
  "outputDirectory": "dashboard/dist",
  "functions": {
    "api/events.ts": {
      "maxDuration": 60
    },
    "api/test-bot.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
